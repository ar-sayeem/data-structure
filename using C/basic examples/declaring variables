#include<stdio.h>
#include<stdlib.h>

struct student
{
    int id;
    float cgpa;
};

int main()
{
    struct student s1, s2;
    s1.id = 5505;
    s1.cgpa = 3.57;

    s2.id = 5553;
    s2.cgpa = 3.41;

    printf("ID of Student-1 was %d and CGPA was %.2f.\n\n", s1.id, s1.cgpa);
    printf("ID of Student-2 was %d and CGPA was %.2f.\n\n", s2.id, s2.cgpa);

    return 0;
}

//Output:

//ID of Student-1 was 5505 and CGPA was 3.57.

//ID of Student-2 was 5553 and CGPA was 3.41.
